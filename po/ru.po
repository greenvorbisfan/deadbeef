#: ../src/main.c:139
#, c-format
msgid "Usage: deadbeef [options] [--] [file(s)]\n"
msgstr "Пример: deadbeef [options] [--] [file(s)]\n"

#: ../src/main.c:140
#, c-format
msgid "Options:\n"
msgstr "Опции:\n"

#: ../src/main.c:141
#, c-format
msgid "   --help  or  -h     Print help (this message) and exit\n"
msgstr "  --help  or  -h     Вывод этого сообщения и выход\n"

#: ../src/main.c:142
#, c-format
msgid "   --quit             Quit player\n"
msgstr "   --quit             Выход\n"

#: ../src/main.c:143
#, c-format
msgid "   --version          Print version info and exit\n"
msgstr "  --version          Вывод версии и выход\n"

#: ../src/main.c:144
#, c-format
msgid "   --play             Start playback\n"
msgstr "   --play             Начать воспроизведение\n"

#: ../src/main.c:145
#, c-format
msgid "   --stop             Stop playback\n"
msgstr "   --stop             Останов\n"

#: ../src/main.c:146
#, c-format
msgid "   --pause            Pause playback\n"
msgstr "   --pause            Пауза\n"

#: ../src/main.c:147
#, c-format
msgid "   --toggle-pause     Toggle pause\n"
msgstr "  --toggle-pause     Воспроизведение/пауза\n"

#: ../src/main.c:148
#, c-format
msgid ""
"   --play-pause       Start playback if stopped, toggle pause otherwise\n"
msgstr ""
"   --play-pause       Воспроизвести, если остановлено (иначе воспроизведение/"
"пауза)\n"

#: ../src/main.c:149
#, c-format
msgid "   --next             Next song in playlist\n"
msgstr "   --next             Следующий трек в плейлисте\n"

#: ../src/main.c:150
#, c-format
msgid "   --prev             Previous song in playlist\n"
msgstr "   --prev             Предыдущий трек в плейлисте\n"

#: ../src/main.c:151
#, c-format
msgid "   --random           Random song in playlist\n"
msgstr "   --random           Случайный трек в плейлисте\n"

#: ../src/main.c:152
#, c-format
msgid "   --queue            Append file(s) to existing playlist\n"
msgstr "   --queue            Добавить файл(ы) в текущий плейлист\n"

#: ../src/main.c:153
#, c-format
msgid ""
"   --gui PLUGIN       Tells which GUI plugin to use, default is \"GTK2\"\n"
msgstr "   --gui PLUGIN       Выбрать GUI-плагин, по умолчанию GTK2\n"

#: ../src/main.c:154
#, c-format
msgid "   --nowplaying FMT   Print formatted track name to stdout\n"
msgstr "   --nowplaying FMT   Вывод инфо о треке в stdout\n"

#: ../src/main.c:157
#, c-format
msgid ""
"                      FMT %%-syntax: [a]rtist, [t]itle, al[b]um,\n"
"                      [l]ength, track[n]umber, [y]ear, [c]omment,\n"
"                      copy[r]ight, [e]lapsed\n"
msgstr ""
"                      FMT %%-синтаксис: [a]rtist, [t]itle, al[b]um,\n"
"                      [l]ength, track[n]umber, [y]ear, [c]omment,\n"
"                      copy[r]ight, [e]lapsed\n"

#: ../src/main.c:160
#, c-format
msgid ""
"                      example: --nowplaying \"%%a - %%t\" should print "
"\"artist - title\"\n"
msgstr ""
"                      пример: --nowplaying \"%%a - %%t\" выведет \"artist - "
"title\"\n"

#: ../src/main.c:163
#, c-format
msgid "                      for more info, see %s\n"
msgstr "                      для получения дополнительной информации см. %s\n"

#: ../src/main.c:165
#, c-format
msgid "                      NOTE: --nowplaying is deprecated.\n"
msgstr "                      ВАЖНО: --nowplaying устарел.\n"

#: ../src/main.c:168
#, c-format
msgid ""
"   --nowplaying-tf FMT  Print formatted track name to stdout, using the new "
"title formatting\n"
msgstr ""
"   --nowplaying-tf FMT  Вывод инфо о треке в stdout с использованием нового "
"форматирования\n"

#: ../src/main.c:171
#, c-format
msgid ""
"                      FMT syntax: http://github.com/DeaDBeeF-Player/deadbeef/"
"wiki/Title-formatting-2.0\n"
msgstr ""
"                      Синтаксис нового форматирования: http://github.com/"
"DeaDBeeF-Player/deadbeef/wiki/Title-formatting-2.0\n"

#: ../src/main.c:174
#, c-format
msgid ""
"                      example: --nowplaying-tf \"%%artist%% - %%title%%\" "
"should print \"artist - title\"\n"
msgstr ""
"                      пример: --nowplaying-tf \"%%artist%% - %%title%%\" "
"выведет \"artist - title\"\n"

#: ../src/main.c:175
#, c-format
msgid "   --volume [NUM]     Print or set deadbeef volume level.\n"
msgstr ""
"  --volume [NUM]     Вывод или установка уровня громкости проишрывателя.\n"

#: ../src/main.c:178
#, c-format
msgid ""
"                      The NUM parameter can be specified in percents "
"(absolute value or increment/decrement)\n"
msgstr ""
"                     Параметр NUM может быть указан в процентах (абсолютное "
"значение или приращение/уменьшение)\n"

#: ../src/main.c:179
#, c-format
msgid "                      or in dB [-50, 0] (if with suffix).\n"
msgstr "                      или в децибелах [-50, 0].\n"

#: ../src/main.c:180
#, c-format
msgid ""
"                      Examples: --volume 80, --volume +10, --volume -5 or --"
"volume -20dB\n"
msgstr ""
"                      Примеры: --volume 80, --volume +10, --volume -5 или --"
"volume -20dB\n"

#: ../src/main.c:183
#, c-format
msgid ""
"   --plugin=[PLUG]    Send commands to a specific plugin. Use PLUG=main to "
"send commands to deadbeef itself.\n"
msgstr ""
"   --plugin=[PLUG]    Передача команд конкретному плагину. Используйте "
"PLUG=main для передачи команд самому проигрывателю.\n"

#: ../src/main.c:184
#, c-format
msgid ""
"                      To get plugin specific commands use --plugin=[PLUG] --"
"help\n"
msgstr ""
"                      Для листинга команд конкретного плагина используйте --"
"plugin=[PLUG] --help\n"

#: ../src/main.c:187
#, c-format
msgid ""
"   --plugin-list      List all available plugins including indication for "
"plugins that support commands.\n"
msgstr ""
"   --plugin-list      Список доступных плагинов с указанием плагинов, "
"поддерживающих команды.\n"

